{
  "android": {
    "root": "android",
    "dependencies": [
      "com.verygoodsecurity:vgscollect:1.6.1",
      "com.verygoodsecurity:adapter-cardio:1.0.2",
      "com.facebook.react:react-native:0.63.4"
    ]
  },
  "ios": {
    "copy": [
      {
        "dest": "{{{projectName}}}/Libraries/Vgs",
        "source": "ios/*"
      }
    ],
    "pbxproj": {
      "addHeaderSearchPath": [
        "\"$(SRCROOT)/{{{projectName}}}/Libraries/Vgs/**\""
      ],
      "addProject": [
        {
          "group": "Libraries",
          "path": "Vgs/Vgs.xcodeproj",
          "staticLibs": [
            {
              "name": "libVgs.a",
              "target": "Vgs"
            }
          ]
        }
      ]
    }
  }
}
